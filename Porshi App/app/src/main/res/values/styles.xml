<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ListView">
        <item name="android:background">@color/listbg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:cacheColorHint">@color/listbg</item>
        <item name="android:divider">@color/listdiv</item>
        <item name="android:dividerHeight">1.0dip</item>
    </style>
    <style name="ContactList">
        <!-- <item name="android:background">@color/listbg</item> -->
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:cacheColorHint">@color/listbg</item>
        <item name="android:divider">@color/listdiv</item>
        <item name="android:dividerHeight">1.0dip</item>
    </style>
    <style name="Animations" />
    <style name="Animations.PopDownMenu" parent="@style/Animations" />
    <style name="Animations.PopDownMenu.Left" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/maxi_start_topleft_to_bottomright</item>
        <item name="android:windowExitAnimation">@anim/min_start_bottomright_to_topleft</item>
    </style>
    <style name="Animations.PopDownMenu.Right" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/maxi_start_topright_to_bottomleft</item>
        <item name="android:windowExitAnimation">@anim/min_start_bottomleft_to_topright</item>
    </style>
    <style name="Animations.PopDownMenu.Center" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/maxi_start_top</item>
        <item name="android:windowExitAnimation">@anim/min_start_bottom</item>
    </style>
    <style name="Animations.PopUpMenu" parent="@style/Animations" />
    <style name="Animations.PopUpMenu.Left" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/maxi_start_bottomleft_to_topright</item>
        <item name="android:windowExitAnimation">@anim/min_start_topright_to_bottomleft</item>
    </style>
    <style name="Animations.PopUpMenu.Right" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/maxi_start_bottomright_to_topleft</item>
        <item name="android:windowExitAnimation">@anim/min_start_topleft_to_bottomright</item>
    </style>
    <style name="Animations.PopUpMenu.Center" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/maxi_start_bottom</item>
        <item name="android:windowExitAnimation">@anim/min_start_top</item>
    </style>

</resources>
